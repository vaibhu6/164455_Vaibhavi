package com.stream.classes;

import java.util.ArrayList;

public class Test {
	public static void main(String[] args) {
		ArrayList<Fruit> fruit = new ArrayList<Fruit>();
		fruit.add(new Fruit("Mango",1500,250,"Yellow"));
		fruit.add(new Fruit("Apple",1500,250,"Red"));
		fruit.add(new Fruit("Strawberry",1500,250,"Red"));
		fruit.add(new Fruit("Pineapple",1500,250,"Yellow"));
		fruit.add(new Fruit("Watermelon",1500,250,"Green"));
		fruit.add(new Fruit("Guava",1500,250,"Yellow"));
		fruit.add(new Fruit("Banana",1500,250,"Yellow"));
		fruit.add(new Fruit("Orange",1500,250,"Orange"));
		fruit.add(new Fruit("Lichi",1500,250,"White"));
		fruit.add(new Fruit("Grape",1500,250,"Green"));
	}
}
