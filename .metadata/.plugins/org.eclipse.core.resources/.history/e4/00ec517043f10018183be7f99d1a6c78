package com.vaibhu.cf;

import java.util.TreeSet;

class Person implements Comparable <TreeSet>{
	
	double weight;
	double height;
	String name;
	
	public Person(double weight, double height, String name) {
		super();
		this.weight = weight;
		this.height = height;
		this.name = name;
	}

	@Override
	public int compareTo(TreeSet person) {
		if(weight>person.weight){
			return 1;
		}
		else if(weight<person.weight)
		{
			return -1;
		}
		else if(weight==person.weight)
		{
			if(height>person.height){
				return 1;
			}
			else if(height<person.height){
				return -1;
			}
		}
	}
	
	
}