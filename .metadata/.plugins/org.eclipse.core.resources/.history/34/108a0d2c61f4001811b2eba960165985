package com.lambda.five;

import java.util.ArrayList;

class Word{
	public static void main(String[] args) {
		
		ArrayList<String> list =new ArrayList<String>();
		list.add("Sanjay");
		list.add("Ulka");
		list.add("Vaibhavi");
		list.add("Neha");
		
	for(String p:list){
		System.out.println(p.charAt(0));
	}
			
		
	}
}