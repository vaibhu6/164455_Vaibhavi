
public class Equals {
	
	String s1="vaibhu";
	String s2="vaibhu";
	String s3= new String("vaibhu");
	
	public boolean test(){
		return s1==s2;
	}
	public boolean test1(){
		return s1==s3;
	}
	public boolean test2(){
		return s1.equals(s2);
	}
	public boolean test3(){
		return s1.equals(s3);
	}
	public static void main(String[] args) {
		
		Equals e= new Equals();
		e.test();
		e.test1();
		e.test2();
		e.test3();
	}
}
