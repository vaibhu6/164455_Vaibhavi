package com.generic;

import java.util.ArrayList;

public class MyList<T>{

	ArrayList<T> list= new ArrayList<T>();

	public void add(T a){
		list.add(a);
	}
	public void remove(T a){
		list.remove(a);	
	}
	public void get(int i){
		list.get(i);
	}
	public T getAll(){
		System.out.println(list);
		
	}
}

