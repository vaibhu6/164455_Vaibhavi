package com.java8;

public class Greeting{
	
	public static void main(String[] args) {
		Greeter gre= () -> System.out.println("Hello World");
		gre.greet();
		Greeter a = () -> System.out.println("Without parameter");
		a.greet();
		ABC ab = (int b) -> System.out.println(b * 2);
		ab.greeting(2);
		
	}
}