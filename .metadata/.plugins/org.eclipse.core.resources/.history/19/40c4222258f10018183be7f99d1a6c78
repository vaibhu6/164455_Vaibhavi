package com.vaibhu.cf;

public class Date {

	private static final char[] date = null;
	public int dd;
	public int mm;
	public int yyyy;
	
	public Date(int dd, int mm, int yyyy) {
		super(); 
		this.dd = dd;
		this.mm = mm;
		this.yyyy = yyyy;
	}

	@Override
	public String toString() {
		return Date = dd=" + dd + ", mm=" + mm + ", yyyy=" + yyyy + ";
	}
	
	public static void main(String[] args) {
		System.out.println(date);
	}
}
 