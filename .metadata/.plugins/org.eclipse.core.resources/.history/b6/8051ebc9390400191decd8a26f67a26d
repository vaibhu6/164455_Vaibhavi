package com.vaibhu.REST.REST_Assg5.dao;

import java.util.ArrayList;
import java.util.List;

import com.vaibhu.REST.REST_Assg5.model.Employee;

public class EmployeDao {

	public List<Employee> getEmployee(){
		ArrayList<Employee> list= new ArrayList<Employee>();
		list.add(new Employee( 1, "", "", "", 500000));
		list.add(new Employee( 2, "", "", "", 800000));
		list.add(new Employee( 3, "", "", "", 100000));
		list.add(new Employee( 4, "", "", "", 90000));
		
		return list;
	}
}
