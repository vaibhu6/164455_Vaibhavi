package com.lambda.four;

import java.util.*;
import java.util.Collection.removeIf;

public class Words {
	public static void main(String[] args) {
		
		List<String> list = Arrays.asList("Apple", "Mango", "Banana");
		list.removeIf(n -> (n.length() % 2 == 0 ));
	
		for(String s : list){
			System.out.println(s);
		}
	
	}
}
