		fruit.add(new Fruit("Guava",90,250,"Yellow"));
		fruit.add(new Fruit("Banana",125,250,"Yellow"));
		fruit.add(new Fruit("Orange",110,250,"Orange"));
		fruit.add(new Fruit("Lichi",60,250,"White"));
		fruit.add(new Fruit("Grape",98,250,"Green"));
