package com.lambda.two;

import java.util.ArrayList;
import java.util.Arrays;


public class Orders {
	public static void main(String[] args) {
		ArrayList<Order> list = (ArrayList<Order>) Arrays.asList(
				new Order(10000,"ACCEPTED"),
				new Order(9000,"COMPLETED"),
				new Order(12000,"REJECTED"),
				new Order(1000,"REJECTED"),
				new Order(15000,"ACCEPTED")
				);
	
		
		list.forEach((i)->{
			if(i.getOrder() >10000 && i.getStatus() == "ACCEPTED" || i.getStatus() == "COMPLETED"){
				System.out.println(i);
			}
		});
	}
}
