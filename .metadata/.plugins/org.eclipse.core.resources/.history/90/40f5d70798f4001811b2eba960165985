		trader.add(new Trader("Kajal","Kolhapur"));
		trader.add(new Trader("Kajal","Pune"));
		trader.add(new Trader("Vaibhavi","Bangalore"));
		trader.add(new Trader("Vinit","Mumbai"));
		trader.add(new Trader("Abhay","Bangalore"));
		
		 trader.stream()
